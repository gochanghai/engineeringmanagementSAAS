<!--产值登记-->
<view class='container'>
  <view class='information'>
    <view class='information_item'>
      <text style='width: 290rpx;'>产值表上传时间</text>
      <picker mode="date" start="2010-01-01" end="2029-01-01" bindchange="InputBeginDate">
        <view class="picker" style='font-size:32rpx; color:#999;'>
          {{updateAt}}
        </view>
      </picker>
      <image src='/images/xia.png' class='down_img'></image>
    </view>
    <view class='information_item'>
      <text>产值 (元)</text>
      <input type='text' confirm-type="next" placeholder='请输入项目名称' placeholder-class='input_class' />
    </view>
    <view class='information_item'>
      <text>可收款 (天)</text>
      <input type='text' confirm-type="next" placeholder='请输入合同编号' placeholder-class='input_class' />
    </view>
    <view class='information_item'>
      <text>甲方确认时间时间</text>
      <picker mode="date" start="2010-01-01" end="2029-01-01" bindchange="InputBeginDate">
        <view class="picker" style='font-size:32rpx; color:#999;'>
          {{confirmdateAt}}
        </view>
      </picker>
      <image src='/images/xia.png' class='down_img'></image>
    </view>
  </view>
  <view class='btom-btns'>
    <view class='ignore' bindtap='cancel'>取消</view>
    <view class='solve' bindtap='commitOutputVal'>确定</view>
  </view>
</view>