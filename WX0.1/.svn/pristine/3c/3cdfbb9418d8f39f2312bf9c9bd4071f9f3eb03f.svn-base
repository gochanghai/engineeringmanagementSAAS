<!--产值表-->
<view class='container'>
  <view class='top-info'>
    <image src='/images/value_bg.png'></image>
    <view class='number-info'>
      <view class='top-total'>
        <text class='txt'>合同金额 (元)</text>
        <text class='val'>{{totalAmount}}</text>
      </view>

      <view class='perform-info'>
        <!-- <view bindtap='confirmRegistration'>
          <image src='/images/chanzhi.png'></image>产值登记
        </view> -->
        <!-- <view bindtap='uploadFile'>
          <image src='/images/sc_white.png' style='width:30rpx;height:28rpx;'></image>上传附件
        </view> -->
         <view bindtap='confirmRegistration'>
          <image src='/images/chanzhi.png'></image>产值登记
        </view>
      </view>
    </view>
  </view>

  <view class='data_content'>
    <view class='list-data' wx:for="{{list}}" wx:key="{{index}}"  bindtap='confirmRegistration'>
      <view>上传时间：{{item.confirmAt}}</view>
      <view>产值总额 (元)：{{item.outputValue}}</view>
      <view>可收款总额 (元)：{{item.receivableAmount}}</view>
      <view class='line'></view>
      <image src='/images/details.png'></image>
    </view>
  </view>
</view>