// pages/projectview/projectview.js
Page({
  data: {
    scrollY: false,
    progressRadius: '40',
    swiperIndex: "0",
    projectList: [{
      id: 0,
      projectName: '腾讯大厦',
    }, {
      id: 1,
      projectName: '汉京大厦',
    }, {
      id: 2,
      projectName: '达实二扩',
    }],

    activeList: {
      imagesel: '/images/building_sel.png',
      image: '/images/building_nor.png',
      color: '#fff',
      fontSize: '27rpx'
    }
  },

  selProject(event) {
    let index = event.currentTarget.dataset.index;
    this.setData({
      swiperIndex: index
    })
  },

  currentFun(event) {
    this.setData({
      swiperIndex: event.detail.current
    })
  },

  navPersonnel(event) {
    let mtype = event.currentTarget.dataset.type;
    wx.navigateTo({
      url: '/pages/managepersonnel/managepersonnel?typeID=' + mtype
    })
  },

  addProject() {
    wx.navigateTo({
      url: '/pages/addproject/addproject',
    })
  },

  projectSet() {
    wx.navigateTo({
      url: '/pages/projectsetting/projectsetting',
    })
  },

  projectNode() {
    wx.navigateTo({
      url: '/pages/projectnode/projectnode',
    })
  },

  declareTime() {
    wx.navigateTo({
      url: '/pages/declaretime/declaretime',
    })
  },

  confirmValue() {
    wx.navigateTo({
      url: '/pages/confirmvalue/confirmvalue',
    })
  },

  receivable() {
    wx.navigateTo({
      url: '/pages/receivable/receivable',
    })
  },

  messageCenter() {
    wx.switchTab({
      url: '/pages/message/message',
    })
  }
})