<!--项目List-->
<view class='container'>
  <scroll-view scroll-y style='height:{{WinHeight}}'>
    <view class='project-list'>

      <view class='project-item' wx:for="{{projectList}}">
        <!-- <text class='status' wx:if="{{item.status == null}}">进行中</text> -->
        <text class='status'>进行中</text>
        <view class='project-item-info'>
          <text>{{item.ProjectAbbreviation}}</text>
          <view class="list1">
            <view class='info'>
              <view>总产值</view>
              <text>￥{{item.outputValue}}.00</text>
            </view>
            <view class='info'>
              <view>总回款</view>
              <text>￥{{item.receivableAmount}}.00</text>
            </view>
            <view class='info'>
              <view>可收款</view>
              <text>￥{{item.actualReceivAmount}}.00</text>
            </view>
            <view class='info'>
              <view>管理人员</view>
              <text>{{item.managerPer}}人</text>
            </view>
            <view class='info'>
              <view>施工人员</view>
              <text>{{item.constructionPer}}人</text>
            </view>
            <view class='info'>
              <view>施工班组</view>
              <text>{{item.group}}组</text>
            </view>
          </view>
        </view>
      </view>

