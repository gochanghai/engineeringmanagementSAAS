/* pages/uploadfile/uploadfile.wxss */

.container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #f0f0f0;
}

.file_list {
  display: flex;
  justify-content: space-between;
  position: relative;
  margin: 0 auto;
  margin-top: 40rpx;
  width: 630rpx;
  height: 300rpx;
  border-radius: 12rpx;
  background-color: #fff;
}

.file_list image.img-type {
  width: 600rpx;
  height: 300rpx;
  border-radius: 12rpx;
}

.file_list view {
  position: absolute;
  height: 60rpx;
  top: 50%;
  right: 50rpx;
  margin-top: -30rpx;
  display: flex;
  align-items: center;
}

.file_list view image {
  width: 38rpx;
  height: 30rpx;
}

.file_list view text {
  font-size: 30rpx;
  color: #39f;
}
